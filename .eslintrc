{
  "parser": "babel-eslint",
  "extends": [
    "prettier",
    "airbnb",
    "plugin:jest/recommended"
  ],
  "plugins": [
    "prettier",
    "jest"
  ],
  "env": {
    "jest": true,
    "browser": true
  },
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-filename-extension": "off"
  }
}
