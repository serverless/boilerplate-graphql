{
  "name": "serverless-boilerplate",
  "custom": {
    "meta": {
      "name": "${meta_bucket}",
      "region": "${meta_bucket_region}"
    },
    "cors": {
      "allowOrigin": "*",
      "allowHeaders": [
        "Content-Type",
        "X-Amz-Date",
        "Authorization",
        "X-Api-Key"
      ]
    }
  },
  "plugins": [
    "serverless-runtime-babel",
    "serverless-meta-sync",
    "serverless-cors-plugin",
    "serverless-client-s3"
  ]
}