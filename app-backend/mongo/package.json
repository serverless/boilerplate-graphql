    "base-64": "^0.1.0",
    "bluebird": "^3.5.1",
    "fs": "0.0.1-security",
    "graphql": "0.10.5",
    "graphql-playground-middleware": "^1.1.2",
    "graphql-playground-middleware-lambda": "^1.4.0",
    "graphql-tools": "2.7.2",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
